{
    "@context": [
        "https://www.w3.org/2022/wot/td/v1.1",
        {
            "@language": "en",
            "ul": "http://18.197.93.117/useless-web-token#"
        }
    ],
    "id": "urn:dev:ops:32473-meta-1",
    "title": "Meta-Interaction Thing",
    "description": "Offers meta interactions through Forms at the Thing level",
    "securityDefinitions": {
        "none": {
            "scheme": "nosec"
        },
        "useless": {
            "scheme": "bearer",
            "format": "ul:uwt"
        },
        "useful": {
            "scheme": "digest"
        }
    },
    "security": "none",
    "base": "http://18.197.93.117/",
    "properties": {
        "one": {
            "type": "string",
            "forms": [{
                "href": "/properties/one",
                "contentType": "text/plain; charset=UTF-8"
            }]
        },
        "other": {
            "type": "string",
            "forms": [{
                "op": [
                    "readproperty",
                    "writeproperty"
                ],
                "href": "/properties/other",
                "contentType": "text/plain; charset=UTF-8"
            }]
        },
        "unsecure": {
            "type": "string",
            "readOnly": true,
            "forms": [{
                "security": "useless",
                "op": "readproperty",
                "href": "/properties/unsecure",
                "contentType": "text/plain; charset=UTF-8"
            }]
        },
        "secure": {
            "type": "string",
            "readOnly": true,
            "forms": [{
                "security": "useful",
                "op": "readproperty",
                "href": "/properties/secure",
                "contentType": "text/plain; charset=UTF-8"
            }]
        }
    },
    "forms": [{
        "op": "readallproperties",
        "href": "/properties/",
        "htv:method": "GET"
    }, {
        "op": "writeallproperties",
        "href": "/properties/",
        "htv:method": "PUT"
    }, {
        "op": "readmultipleproperties",
        "href": "/properties/",
        "htv:method": "POST"
    }, {
        "op": "writemultipleproperties",
        "href": "/properties/",
        "htv:method": "PATCH"
    }]
}
