{
    "id": "urn:actionResponse2",
    "@context":"https://www.w3.org/2022/wot/td/v1.1",
    "title": "MyLampThing",
    "description": "Valid TD showing how to specify another content type in an action response",
    "securityDefinitions": {
        "basic_sc": {
            "scheme": "basic",
            "in": "header"
        }
    },
    "security": ["basic_sc"],
    "schemaDefinitions":{
        "mySchema":{
            "type":"string",
            "readOnly":true,
            "writeOnly":false
        }
    },
    "actions": {
        "crop": {
            "idempotent":false,
            "safe":false,
            "forms": [{
                "href": "http://upsq1c.local:9191/api/frame/crop",
                "contentType": "application/json",
                "security": ["basic_sc"],
                "op": ["invokeaction"],
                "htv:methodName": "POST",
                "response":{
                    "contentType":"image/jpeg"
                },
                "additionalResponses": [
                    {
                        "success":true,
                        "contentType": "application/json",
                        "schema":"mySchema"
                    },
                    {
                        "success":false,
                        "contentType": "application/json"
                    }
                ]
            }]
        }
    }
}